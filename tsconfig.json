{
    "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": false,
      "allowJs": true,
      "moduleResolution": "node"
    },
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended"
}